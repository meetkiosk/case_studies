generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id String @id

  firstName String @map("first_name")
  lastName  String @map("last_name")

  @@map("user")
}

model Task {
  id Int @id @default(autoincrement())
  title String
  state String
  owner String
  description String
}
